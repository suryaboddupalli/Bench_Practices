{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": 0,
        "no-empty": 1,
        "no-irregular-whitespace": 1,
        "@typescript-eslint/no-var-requires": 1
    }
}